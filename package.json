{
  "name": "performance-masterclass-2018-10",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "prebuild": "rimraf cache/ && node scripts/copy.js",
    "build": "run-s build:*",
    "build:css": "node scripts/styles.js",
    "build:js": "node scripts/scripts.js",
    "postbuild": "run-s revision compress",
    "compress": "node scripts/compress.js",
    "dev": "run-p start watch",
    "prestart": "npm run build",
    "proxy": "npm-run-all --parallel proxy:*",
    "proxy:local": "browser-sync start --proxy 'localhost:6521' --port 6524",
    "proxy:public": "ngrok http 6521",
    "revision": "node scripts/revision-hash.js",
    "start": "nodemon --expose-http2 server.js",
    "watch": "chokidar 'src/**' --command 'npm run build'"
  },
  "author": "De Voorhoede",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "^1.2.1",
    "critical": "^1.3.4",
    "fs-extra": "^7.0.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-brotli": "^1.2.1",
    "gulp-concat": "2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-filter": "^5.1.0",
    "gulp-gzip": "^1.4.2",
    "gulp-rev": "^9.0.0",
    "gulp-rev-css-url": "0.1.0",
    "gulp-rev-replace": "^0.4.4",
    "gulp-uglify": "^3.0.1",
    "ncp": "2.0.0",
    "ngrok": "^3.1.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cheerio": "1.0.0-rc.2",
    "cookie-parser": "1.4.3",
    "express": "^4.16.4",
    "helmet": "^3.14.0",
    "html-minifier": "^3.5.20",
    "mime": "^2.3.1",
    "npm-run-all": "^4.1.3",
    "nunjucks": "^3.1.3",
    "request": "^2.88.0",
    "server-timings": "^2.0.1",
    "shrink-ray-current": "^2.1.2",
    "spdy": "3.4.7"
  }
}
